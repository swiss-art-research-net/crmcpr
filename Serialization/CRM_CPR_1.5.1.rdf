    <rdfs:label xml:lang="en">stipulates type of object outcome</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CP5</skos:notation>
    <rdfs:domain rdf:resource="https://takin.solutions/ontologies/CRMcpr/CC12_Event_Trigger_Template"/>
  </rdf:Property>
